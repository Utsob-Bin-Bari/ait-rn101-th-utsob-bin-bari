{
  "name": "AsthaIt",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "server": "json-server --watch db.json --port 3000 --host 0.0.0.0 --middlewares ./server-middleware.js",
    "open": "open ios/AsthaIt.xcworkspace",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && pod deintegrate && pod install && cd ..",
    "pod-start": "cd ios && bundle install && bundle exec pod install && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "build-apk-debug": "yarn clean-android && cd android && ./gradlew assembleDebug && cd ..",
    "build-apk-release": "yarn clean-android && cd android && ./gradlew assembleRelease && cd ..",
    "build-ios-debug": "yarn clean-ios && react-native run-ios --configuration Debug",
    "build-ios-release": "yarn clean-ios && react-native run-ios --configuration Release",
    "archive-ios": "cd ios && xcodebuild -workspace AsthaIt.xcworkspace -scheme AsthaIt -configuration Release archive -archivePath ./build/AsthaIt.xcarchive"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.3",
    "@reduxjs/toolkit": "^2.10.1",
    "axios": "^1.13.2",
    "diff-match-patch": "^1.0.5",
    "react": "19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.82.1",
    "react-native-blob-util": "^0.23.2",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-image-picker": "^8.2.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^15.14.0",
    "react-native-worklets": "^0.6.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "json-server": "^0.17.4",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
